#include<iostream>
#include<string>
using namespace std;

int main()
{
    string s;
    while(getline(cin,s))
    {
        int count[256]={0};
        int flag=0;
        for(int i=0;i<s.size();++i)
        {
            count[s[i]]++;
        }
        for(int j=0;j<s.size();++j)
        {
            if(count[s[j]]==1)
            {
                cout << s[j] << endl;
                flag=1;
                break;
            }
        }
        if(flag!=1)
            cout << -1 << endl;
    }
    return 0;
}
